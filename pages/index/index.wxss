.list li {
    width:100%;
    height: auto;
    min-height:100px;
    border-bottom:1px solid #e5e5e5;
    display: block;
    vertical-align: top;
}
.list li .date {
    width: 710rpx;
    color:#c4c4c5;
    font-size:14px;
    font-weight:500;
    background:#f5f5f6;
    height:24px;
    line-height:24px;
    padding:0 20rpx;
    display: block;
}

.list li .contents {
    width: 710rpx;
    height: auto;
    min-height: 76px;
    display: flex;
	flex-direction: row;
}

.list li .contents>div {
    color: #c4c4c4;
    display: inline-block;
}

.list li .contents .pic {
    width: 70px;
    height: 70px;
}
.list li .contents .pic img {
    width: 100%;
    height: 100%;
}

.list li .contents .text {
    padding: 5px 0;
    margin-left: 10px;
    flex: 1;
    display: flex;
	flex-direction: column;
}

.list li .contents .text >div:first-child {
    color: #000;
    font-size: 16px;
    font-weight: 500;
    height: auto;
    min-height: 44px;
    line-height: 22px;
}

.list li .contents .text >div:last-child {
    font-size: 12px;
    line-height: 18px;
}

.list li .contents .times {
    font-size: 12px;
    width: 40px;
    text-align: right;
    margin-top: 10px;
}

.list li .contents .times b {
    color: #303030;
    font-size: 28px;
    letter-spacing: 1px;
    display: block;
}